@model IEnumerable<MoneyTransferWeb.Models.Institution>

@{
    ViewData["Title"] = "Index";
}

<h1>កុងនិងប្រាក់ផ្សេងៗ</h1>

<p>
    <a asp-action="Create">បន្ថែមថ្មី</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.INameKH)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.INameEN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IDetail)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.INameKH)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.INameEN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IDetail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InstitutionID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InstitutionID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InstitutionID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
